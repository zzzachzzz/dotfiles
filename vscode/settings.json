{
    "vim.leader": "<space>",
    "vim.insertModeKeyBindings": [
        {
            "before": ["j", "k"],
            "after": ["<Esc>"]
        }
    ],
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": ["u"],
            "commands": ["undo"]
        },
        {
            "before": ["C-r"],
            "commands": ["redo"]
        }
    ],
    "vim.vimrc.enable": true,
    "vim.vimrc.path": "~/.vimrc-vscode",
    "vim.cursorStylePerMode.insert": "line",
    "editor.cursorBlinking": "solid",
    "editor.cursorStyle": "block",
    "editor.fontSize": 16,
    "terminal.integrated.fontSize": 16,
    "terminal.integrated.fontFamily": "'Inconsolata Nerd Font Mono', Consolas, 'Courier New', monospace",
    "editor.tabSize": 2,
    "files.exclude": {
        "node_modules": true
    },
    "files.trimFinalNewlines": true,
    "explorer.openEditors.visible": 0,
    "extensions.ignoreRecommendations": false,
    "editor.minimap.enabled": false,
    "editor.codeLens": false,
    "workbench.startupEditor": "none",
    "workbench.list.automaticKeyboardNavigation": false,
    "editor.renderControlCharacters": false,
    "editor.matchBrackets": "never",
    "editor.guides.indentation": false,
    "editor.cursorSurroundingLines": 4,
    "editor.fontFamily": "'Inconsolata Nerd Font Mono', Consolas, 'Courier New', monospace",
    "editor.acceptSuggestionOnCommitCharacter": false,
    "todohighlight.isEnable": true,
    "terminal.integrated.defaultProfile.windows": "Git Bash",
    "git.openRepositoryInParentFolders": "never",
    "workbench.editor.enablePreview": false,
    "workbench.colorTheme": "Monokai Custom Zach"
}
